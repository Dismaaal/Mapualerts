<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Project.ConsultPage">
  <StackLayout Orientation="Vertical">
  <Label Text="Consultation" VerticalOptions="Center" HorizontalOptions="Center" />
  <Entry x:Name="SendTo" Placeholder="Send to"></Entry>
  <Editor x:Name="Msg" ></Editor>
  <Button Text ="SMS" Clicked="btnSMS_Clicked"/>
  <Button Text ="Call" Clicked="btnCall_Clicked"/>
  <Button Text ="Email" Clicked ="btnEmail_Clicked"/>
    </StackLayout>

</ContentPage>