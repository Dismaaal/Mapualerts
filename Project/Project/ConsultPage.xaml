<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Project.ConsultPage"
             BackgroundColor="White">
  <StackLayout Orientation="Vertical">
  <Label Text="Consultation" VerticalOptions="Center" HorizontalOptions="Center" TextColor="Black"/>
  <Entry x:Name="SendTo" Placeholder="Send to" PlaceholderColor="Gray" TextColor="Black"></Entry>
  <Entry x:Name="Msg" Placeholder="Message" PlaceholderColor="Gray" VerticalOptions="FillAndExpand" HorizontalOptions="Fill" TextColor="Black"></Entry>
  <Button Text ="SMS" Clicked="btnSMS_Clicked"/>
  <Button Text ="Call" Clicked="btnCall_Clicked"/>
  <Button Text ="Email" Clicked ="btnEmail_Clicked"/>
    </StackLayout>

</ContentPage>